{
  "name": "@xystemize/twitter-signin",
  "version": "1.4.2",
  "description": "Social Signin with Twitter",
  "keywords": [
    "react",
    "react-native",
    "react native",
    "native",
    "twitter",
    "signin",
    "social signin",
    "authentication"
  ],
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.ts",
  "types": "lib/typescript/index.d.ts",
  "homepage": "https://github.com/xystemize/twitter-signin.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/xystemize/twitter-signin.git"
  },
  "author": "Xystemize <xystemize@gmail.com> (https://github.com/xystemize)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xystemize/react-native-pdf-utils/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@xystemize:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react-native": "^0.65.5",
    "react-native": "^0.66.0",
    "react-native-builder-bob": "^0.18.2",
    "typescript": "^4.4.3",
    "@release-it/conventional-changelog": "^5.0.0",
    "release-it": "^15.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@react-native-community/eslint-config": "^3.0.2",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "release": "release-it"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=4.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "deprecated": false,
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
